<script>
  document.getElementById("species").addEventListener("change", selectedInput)
  const breedDog = document.getElementById("dog")
  const secondBreed = document.getElementById("secondBreed")
  const breedCat = document.getElementById("cat")
  const breedOther = document.getElementById("other")

  function selectedInput(event){
    breedDog.value
    let selection = event.target.value
    if(selection == "Dog"){
      breedDog.classList.remove("hidden")
      secondBreed.classList.remove("hidden")
      breedCat.classList.add("hidden")
      breedOther.classList.add("hidden")
    }
    else if(selection == "Cat"){
      breedDog.classList.add("hidden")
      secondBreed.classList.add("hidden")
      breedCat.classList.remove("hidden")
      breedOther.classList.add("hidden")
    }
    else{
      breedDog.classList.add("hidden")
      secondBreed.classList.add("hidden")
      breedCat.classList.add("hidden")
      breedOther.classList.remove("hidden")
    }
    formReset()
  }
  function formReset(){
    document.getElementById("breedDog").selectedIndex = 0; 
    document.getElementById("secondBreedSelector").selectedIndex = 0; 
    document.getElementById("breedCat").selectedIndex = 0; 
    document.getElementById("breedOther").selectedIndex = 0;
  }

  document.getElementById("microchip").addEventListener("change", showMicrochipNumber)
  const microchipNumberContainer = document.getElementById("microchipNumberContainer")
  const microchipNumber = document.getElementById("microchipNumber")

  function showMicrochipNumber(event){
    let selection = event.target.value
    if(selection == "yes"){
      microchipNumberContainer.classList.remove("hidden")
    }
    else{
      microchipNumberContainer.classList.add("hidden")
      microchipNumber.value = ""
    }
  }

  document.getElementById("wearingCollar").addEventListener("change", showCollarColor)
  const collarColorContainer = document.getElementById("collarColorContainer")
  const collarColor = document.getElementById("collarColor")

  function showCollarColor(event){
    let selection = event.target.value
    if(selection == "yes"){
      collarColorContainer.classList.remove("hidden")
    }
    else{
      collarColorContainer.classList.add("hidden")
      collarColor.selectedIndex = 0
    }
  }


</script>
